{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [ "html" ],
    "settings": {
        "html/html-extensions": [".html", ".we"]
    },
    "rules": {
        "no-console": 0,
        "restrictedNamedExports": [0],
        "no-param-reassign": 0,
        "import/extensions": 0
    }
}